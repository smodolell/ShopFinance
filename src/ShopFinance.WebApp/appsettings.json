{
  "FileUpload": {
    "UploadPath": "wwwroot/uploads",
    "BaseUrl": "/uploads",
    "MaxFileSize": 10485760,
    "AllowedExtensions": [ ".jpg", ".jpeg", ".png", ".pdf", ".docx" ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=App_Data\\ShopFinance.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
